  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-session": "^2.0.49",
    "@vue/cli": "^4.2.2",
    "concurrently": "^9.1.0",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "install-all": "npm install && npm --prefix frontend install && npm --prefix backend install",
    "prisma-generate": "cd backend && npx prisma generate",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm run serve",
    "build": "cd frontend && npm run build && cd ../backend && npm run build",
    "start": "cd backend && npm run start",
    "pm2-start": "cd backend && npm run pm2-start",
    "pm2-stop": "cd backend && npm run pm2-stop",
    "pm2-restart": "cd backend && npm run pm2-restart",
    "pm2-save": "cd backend && npm run pm2-save"
  },
