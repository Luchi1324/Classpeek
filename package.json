    "color-name": "^1.1.4",
    "concurrently": "^7.6.0",
    "cookie-session": "^2.1.0",
    "csstype": "^3.1.3",
    "date-fns": "^2.30.0",
    "emoji-regex": "^8.0.0",
    "entities": "^4.5.0",
    "escalade": "^3.2.0",
    "estree-walker": "^2.0.2",
    "get-caller-file": "^2.0.5",
    "has-flag": "^4.0.0",
    "is-fullwidth-code-point": "^3.0.0",
